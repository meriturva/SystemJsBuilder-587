{
  "compileOnSave": true,
  "compilerOptions": {
    "target": "es5",
    "module": "system",
    "declaration": false,
    "noImplicitAny": false,
    "noEmitOnError": false,
    "removeComments": false,
    "moduleResolution": "node",
    "noLib": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": false,
    "listFiles": false
  },
  "exclude": [
    "node_modules"
  ]
}